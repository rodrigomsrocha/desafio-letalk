generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Loan {
  id              String   @id @default(uuid())
  cpf             String
  uf              String
  dataNascimento  DateTime
  valorEmprestimo Float
  valorParcela    Float
  juros           Float
  qtdParcelas     Int
}
